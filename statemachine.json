{
  "Comment": "Validation + DynamoDB Insert Workflow",
  "StartAt": "ValidateInput",
  "States": {
    
    "ValidateInput": {
      "Type": "Task",
      "Resource": "arn:aws:lambda:REGION:ACCOUNT_ID:function:ValidateInputFunction",
      "ResultPath": "$.validationResult",
      "Next": "WriteToDynamoDB"
    },
    
    "WriteToDynamoDB": {
      "Type": "Task",
      "Resource": "arn:aws:lambda:REGION:ACCOUNT_ID:function:WriteToDynamoDBFunction",
      "ResultPath": "$.dbResult",
      "End": true
    }
    
  }
}
